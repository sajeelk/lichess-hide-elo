{
  "manifest_version": 2,
  "name": "Lichess hide Elo",
  "version": "0.1",
  "description": "Hides Elo ratings on lichess.org",
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["*://*.lichess.org/*", "*://lichess.org/*"],
      "css": ["hideEloInitially.css"]
    },
    {
      "run_at": "document_end",
      "matches": ["*://*.lichess.org/*", "*://lichess.org/*"],
      "js": ["hideElo.js"],
      "css": ["hideElo.css"]
    }
  ]
}
